{
    "name": "game-api",
    "type": "commonjs",
    "version": "0.0.1",
    "description": "Game Backside",
    "author": "Daniel Donahoe daniel.donahoe@gmail.com",
    "private": true,
    "license": "to kill",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "typeorm": "typeorm-ts-node-commonjs",
        "migrate:backend:run": "pnpm run typeorm migration:run -d src/data-source.ts",
        "cli": "node -r ts-node/register ./src/cli/slice.ts",
        "cli:two": "ts-node ./src/cli/string-util.ts",
        "migrate:backend:create": "pnpm run typeorm migration:create src/migrations/PostRefactoring",
        "migrate:reset": "pnpm run typeorm migration:create src/migrations/PostRefactoring",
        "build": "nest build --webpack",
        "start:dev": "nest build --builder webpack --webpackPath webpack-hmr.config.cjs --watch",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "pnpm run start:dev",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --config ../../.eslintrc.json",
        "test": "jest",
        "test:coverage": "jest --coverage --coverageReporters=text --coverageReporters=html --coverageReporters=lcov",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
    },
    "dependencies": {
        "@google-cloud/logging-winston": "^6.0.0",
        "@nestjs/axios": "3.0.3",
        "@nestjs/common": "^10.4.1",
        "@nestjs/config": "^3.2.3",
        "@nestjs/core": "^10.4.1",
        "@nestjs/mapped-types": "*",
        "@nestjs/platform-express": "^10.4.1",
        "@nestjs/platform-socket.io": "^10.4.1",
        "@nestjs/typeorm": "10.0.2",
        "@nestjs/websockets": "^10.4.1",
        "axios": "1.7.7",
        "chalk": "^5.3.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cli-table3": "^0.6.5",
        "cookie": "^0.6.0",
        "deep-freeze-strict": "^1.1.1",
        "js-cookie": "^3.0.5",
        "nest-winston": "^1.9.7",
        "newrelic": "^12.3.1",
        "pg": "^8.12.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "socket.io": "^4.7.5",
        "typeorm": "0.3.20",
        "uuid": "^10.0.0",
        "winston": "^3.14.2",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@commander-js/extra-typings": "^12.1.0",
        "@nestjs/cli": "^10.4.5",
        "@nestjs/testing": "^10.4.1",
        "@types/deep-freeze-strict": "^1.1.2",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/node": "^22.5.4",
        "@types/pg": "^8.11.8",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.4.0",
        "@typescript-eslint/parser": "^8.4.0",
        "commander": "^12.1.0",
        "eslint": "^8.57.0",
        "inquirer": "^11.0.2",
        "jest": "^29.7.0",
        "open": "^10.1.0",
        "run-script-webpack-plugin": "^0.2.0",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-essentials": "^10.0.2",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "5.5.4",
        "webpack": "^5.94.0",
        "webpack-node-externals": "^3.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "src/migrations/"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
