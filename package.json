{
    "name": "game-web",
    "version": "1.0.34",
    "private": true,
    "description": "Game Codebase",
    "repository": {
        "type": "git",
        "url": "https://github.com/dandonahoe/crude-cards.git"
    },
    "browser": {
        "path-to-regexp": false,
        "lapack": false,
        "WNdb": false,
        "fs": false
    },
    "files": [
        "/bin",
        "/dist"
    ],
    "scripts": {
        "commit": "slick-commit",
        "artifact:build": "pnpm job:docs && pnpm test:coverage && pnpm dep:circular && pnpm dep:full && pnpm build:storybook",
        "auth:google": "gcloud auth application-default login",
        "build": "IS_BUILDING=true next build",
        "build:all": "IS_BUILDING=true pnpm build:backend && pnpm build:frontend",
        "build:backend": "IS_BUILDING=true pnpm --prefix src/api run build",
        "build:frontend": "IS_BUILDING=true pnpm build",
        "build:storybook": "IS_BUILDING=true storybook build --output-dir storybook-static",
        "ci-semantic-release": "semantic-release",
        "docker:frontend": "docker build --progress=plain --no-cache -f DockerfileUI -t crude-cards-ui-container .",
        "docker:backend": "docker build --progress=plain --no-cache -f DockerfileAPI -t rude-cards-api-container .",
        "docker:ui": "docker build --progress=plain --no-cache -f DockerfileWeb -t rude-cards-web-container .",
        "job:pull-dev-env": "GPG_TTY=$(tty) ts-node internal/script/pull-env",
        "dep:circular": "DATE=$(date +%Y-%m-%d-%S) && madge --image ./internal/log/$DATE-circular.svg --extensions ts,tsx,js --ts-config ./tsconfig.json internal/script/narrative --circular",
        "dep:full": "DATE=$(date +%Y-%m-%d-%S) && madge --image ./internal/log/$DATE-full.svg --extensions ts,tsx,js --ts-config ./tsconfig.json internal/script/narrative",
        "dev": "next dev",
        "dev:all": "rm -rf node_modules && pnpm install && pnpm lint && pnpm test && pnpm build && code",
        "dev:clerk": "open https://dashboard.clerk.com/apps/app_2LWoPByqissTzZoa4vlEXY6ppk5/instances/ins_2NCz4PyWwDorAJ5HUUnWqN1F0ui",
        "dev:kill": "kill-port 3000",
        "dev:reset": "ts-node internal/script/reset",
        "dev:narrative": "ts-node internal/script/narrative",
        "job:codebase": "ts-node internal/script/codebase",
        "job:disable-snooping": "next telemetry disable",
        "job:docs": "typedoc",
        "job:purge": "ts-node internal/script/purge",
        "job:relock": "rm -f pnpm-lock.yaml src/api/pnpm-lock.yaml && pnpm install && pnpm --prefix=src/api install",
        "lint": "next lint",
        "lint:all": "TZ=GMT pnpm lint:frontend && pnpm lint:backend && pnpm trunk:check",
        "lint:backend": "TZ=GMT pnpm --prefix src/api run lint",
        "lint:frontend": "TZ=GMT pnpm lint",
        "puppet": "ts-node internal/script/puppet",
        "start": "TZ=GMT PORT=8080 next start --port 8080",
        "start:backend": "pnpm --prefix src/api run start",
        "start:frontend": "pnpm start",
        "start:storybook": "http-server storybook-static -p 8080",
        "dev:storybook": "storybook dev -p 6006",
        "test": "TZ=GMT jest",
        "test:all": "TZ=GMT pnpm test:frontend && pnpm test:backend",
        "test:api": "TZ=GMT pnpm --prefix=src/api run test",
        "test:backend": "TZ=GMT pnpm --prefix src/api run test",
        "test:coverage": "TZ=GMT jest --coverage",
        "test:frontend": "TZ=GMT pnpm test",
        "test:watch": "TZ=GMT jest --watch",
        "test:web": "TZ=GMT pnpm test",
        "trunk": "trunk",
        "trunk:check": "trunk check --no-fix --show-existing",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
